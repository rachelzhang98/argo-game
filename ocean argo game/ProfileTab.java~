import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
import java.util.*;


public class ProfileTab extends JPanel 
{ 
  private SaveImage i;
  private JLabel tempImageLabel, salImageLabel;

  public ProfileTab(SaveImage i) 
  { 
    this.i = i;
    tempImageLabel = new JLabel(i.getTempImage());
    salImageLabel = new JLabel(i.getSalImage());
    add(tempImageLabel);
    add(salImageLabel);
  }
}